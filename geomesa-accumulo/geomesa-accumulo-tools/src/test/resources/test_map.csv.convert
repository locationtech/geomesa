{
   type         = "delimited-text",
   format       = "CSV",
   id-field     = "toString($i)",
   fields = [
     { name = "i",       transform = "$1::double" },
     { name = "numbers", transform = "parseMap('int -> string', $2)"},
     { name = "dtg",      transform = "$3::date" },
     { name = "lon",      transform = "$4::double" },
     { name = "lat",      transform = "$5::double" },
     { name = "geom",     transform = "point($lon, $lat)" }
   ]
 }
