{
   type         = "delimited-text",
   format       = "TSV",
   id-field     = "toString($fid)",
   fields = [
     { name = "fid",      transform = "$1::double" },
     { name = "lat",      transform = "$2::double" },
     { name = "lon",      transform = "$3::double" },
     { name = "geom",     transform = "point($lat, $lon)" }
   ]
 }
